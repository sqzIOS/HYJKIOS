#import <Foundation/Foundation.h>

@interface SDGoodsModel : NSObject

@property (nonatomic, copy) NSString *goodsName;    //商品名称

@property (nonatomic, copy) NSString *goodsId;      //商品id

@property (nonatomic, copy) NSString *goodsCategoryId;  //商品分类id

@property (nonatomic, copy) NSString *goodsSn;      //商品序列号

@property (nonatomic, copy) NSString *goods_price;  //商品价格

@property (nonatomic, copy) NSString *goodtype_id;  //商品类别id

