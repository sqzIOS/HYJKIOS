#import "SHHealthSubBottomView.h"

@implementation SHHealthSubBottomView

#pragma mark ---getter setter
-(UIImageView *)topIconImageView
{
    if (_topIconImageView == nil) {
        _topIconImageView  = [[UIImageView alloc]init];
        _topIconImageView.backgroundColor = [UIColor clearColor];
    }
    return _topIconImageView;
}
-(UIImageView *)lineIconImageView
{
    if (_lineIconImageView == nil) {
        _lineIconImageView  = [[UIImageView alloc]init];
        _lineIconImageView.backgroundColor = RGBCOLOR16(0x5edef1);
    }
    return _lineIconImageView;
}
-(UIImageView *)bgIconImageView
{
    if (_bgIconImageView == nil) {
        _bgIconImageView  = [[UIImageView alloc]init];
        _bgIconImageView.backgroundColor = [UIColor clearColor];
    }
    return _bgIconImageView;
}
-(UILabel *)recommendLabel
{
    if (_recommendLabel == nil) {
        _recommendLabel = [[UILabel alloc]init];
        _recommendLabel.backgroundColor = [UIColor clearColor];
        _recommendLabel.textColor = RGBCOLOR16(0x333333);
    }
    return _recommendLabel;
}
-(UILabel *)nearbyLabel
{
    if (_nearbyLabel == nil) {
        _nearbyLabel = [[UILabel alloc]init];
        _nearbyLabel.backgroundColor = [UIColor clearColor];
        _nearbyLabel.textColor = RGBCOLOR16(0x666666);
    }
    return _nearbyLabel;
}
-(UIScrollView *)subScrollView
{
    if (_subScrollView == nil) {
        _subScrollView = [[UIScrollView alloc]init];
        _subScrollView.backgroundColor = [UIColor clearColor];
        _subScrollView.showsHorizontalScrollIndicator = NO;
    }
    return _subScrollView;
}

@end
